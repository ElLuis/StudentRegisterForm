<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.centennial.elluis.studentstaff.MainActivity"
    android:padding="60dp">


    <Button
        android:id="@+id/studentBtn"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Student"
        android:onClick="studentBtn_OnClick"
        style="@style/staffButtonStyle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/staffBtn"
        style="@style/staffButtonStyle"
android:onClick="staffBtn_OnClick"
        android:text="Staff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
